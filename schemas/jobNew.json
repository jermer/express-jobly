{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/jobNew.schema.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "salary": {
            "type": "integer"
        },
        "equity": {
            "type": "number",
            "default": 0
        },
        "companyHandle": {
            "type": "string",
            "minLength": 1,
            "maxLength": 10
        }
    },
    "additionalProperties": false,
    "required": [
        "title",
        "salary",
        "companyHandle"
    ]
}